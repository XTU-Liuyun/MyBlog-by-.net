@{
    Layout = null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>无标题文档</title>
    <link href="/adminstyle/css/style.css" rel="stylesheet" type="text/css" />
    <script language="JavaScript" src="/adminstyle/js/jquery.js"></script>
    <script type="text/javascript">
        $(function () {
            //顶部导航切换
            $(".nav li a").click(function () {
                $(".nav li a.selected").removeClass("selected")
                $(this).addClass("selected");
            })
        })
    </script>


</head>

<body style="background:url(/adminstyle/images/topbg.gif) repeat-x;">

    <div class="topleft">
        <a href="/admin/home/index" target="_parent"><img src="/adminstyle/images/logo.png" title="系统首页" /></a>
    </div>

    <ul class="nav">
        <li><a href="default.html" target="rightFrame" class="selected"><img src="/adminstyle/images/icon01.png" title="工作台" /><h2>工作台</h2></a></li>
    </ul>

    <div class="topright">
        
        <div class="user">
            <span>@base.Context.Session.GetString("adminusername")</span>
            <i style="padding-right:10px;">
                <a href="/admin/login" target="_parent">退出</a>
            </i>
            
        </div>

    </div>
    <script>
        
    </script>
</body>
</html>
